{
	"name": "playground-php-bridge",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/playground/php-bridge/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/vite:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/playground/php-bridge"
			}
		},
		"test": {
			"executor": "@nx/vite:test",
			"outputs": ["{options.reportsDirectory}"],
			"options": {
				"passWithNoTests": true,
				"reportsDirectory": "../../../coverage/packages/playground/php-bridge"
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"packages/playground/php-bridge/**/*.ts",
					"packages/playground/php-bridge/package.json"
				]
			}
		}
	},
	"tags": []
}
